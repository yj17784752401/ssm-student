<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.roadjava.student.mapper.AdminMapper">

    <select id="validateLogin" resultType="com.roadjava.student.bean.entity.AdminDO">
        select *
        from admin
        where admin_name = #{loginName};

    </select>

</mapper>
